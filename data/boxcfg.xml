if nBox == 0
  Aadd( aBoxes, {,,"","",,,,0,0,0,.F.} )
  nBox := Len(aBoxes)
endif
aBox := aBoxes[nBox]
aBox[1] := Trim(m_title)
aBox[2] := Trim(m_addr)
aBox[5] := Trim(m_login)
aBox[6] := Trim(m_pass)
aBox[7] := m_refr
aBox[11] := .F.
lChgConfig := .T.
oBrw1:Refresh()
hwg_EndDialog()
]]>
